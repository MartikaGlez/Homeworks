#include <iostream>
using namespace std;
int main ()
{
  int num1, num2, suma=0;
  cout << "This program will calculate the sum of integers in a range that you provide."<<endl;
  cout << "Introduce the lower integer value: ";
  cin >> num1;
  cout << "Introduce the highest integer value: ";
  cin >> num2;
  do {
    suma = suma + num1;
    num1 = num1+1;
  } while (num1 <= num2);
  cout << "The total sum is: "<<suma;
}
